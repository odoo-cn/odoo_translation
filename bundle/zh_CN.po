"            <td>${line['name']}</td>\n"
"            <td>${line['target']}\n"
"            % if line['suffix']:\n"
"                ${line['suffix']}\n"
"            % endif\n"
"            </td>\n"
"            <td>${line['current']}\n"
"            % if line['suffix']:\n"
"                ${line['suffix']}\n"
"            % endif\n"
"            </td>\n"
"            <td>${line['completeness']} %</td>\n"
"        </tr>\n"
"    % endfor\n"
"    </table>\n"
